---
import { SITE_CONTENT } from "@config";
import Hero from "@components/Hero.astro";
import Layout from "@layouts/Layout.astro";
import Projects from "@components/Projects.astro";
import CTA from "@components/CTA.astro";
import About from "@components/About.astro";
import Testimonials from "@components/Testimonials.astro";
import TechPartners from "@components/TechPartners.astro";
import OurPartners from "@components/OurPartners.astro";

import "../styles/global.css";
---

<Layout>
  <Hero {...SITE_CONTENT.hero} />
  <Projects projects={SITE_CONTENT.projects} />
  <Testimonials testimonials={SITE_CONTENT.testimonials} />
  <OurPartners />
  <TechPartners />
  <CTA />
  <About {...SITE_CONTENT.about} name={SITE_CONTENT.hero.name} />
</Layout>
